.self-center{
  text-align: center;
  font-size: 36rpx;
}
.item-checked{
  background-color: #1AAD19;
  color: white;
  font-weight: bold;
  font-size: 48rpx;
}
.border-white {
  border-bottom: 4rpx solid white;
}
.cu-form-group {
  min-height: 90rpx;
}
.text-self{
  margin-bottom: 12rpx;
  line-height: 32rpx;
}
.text-bold{
  font-weight: bold;
}
.padd-but{
  padding: 20rpx 30rpx 0;
}
.nav .cu-item{
  min-width: 66rpx;
  margin: 0;
  padding: 0;
}
.before-box{
  padding:1rpx 30rpx;
  font-size:30rpx;
  height:60rpx;
  font-family:UICTFontTextStyleBody;
  align-items:center;
  line-height:60rpx;
}
.column {
  flex-basis: 100%;
  display: flex;
  justify-content: space-between;
}
.box {
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
  align-items:center;
  justify-content:space-between;
}
.margin-left{
  margin-left: 20rpx;
}
.delete{
  padding-left: 300rpx;
}
.del-view{
  display: flex;
  justify-content: center;
}
.self-style{
  margin-left: initial;
  margin-top: 10rpx;
  color: red;
  height:76rpx;
  text-decoration: underline;
  background-color: white;
}
.self-style::after{
  border: none;
}